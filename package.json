{
  "name": "lora-manager-frontend",
  "version": "2.1.0",
  "description": "Frontend assets for LoRA Manager - AI-Powered LoRA Management System",
  "scripts": {
    "build:css": "tailwindcss -i ./app/frontend/static/css/input.css -o ./app/frontend/static/css/styles.css --minify",
    "watch:css": "tailwindcss -i ./app/frontend/static/css/input.css -o ./app/frontend/static/css/styles.css --watch",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:css": "npm run watch:css",
    "dev:backend": "uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "prod:build": "npm run build && ENVIRONMENT=production",
    "prod:serve": "ENVIRONMENT=production uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest tests/unit --coverage",
    "test:unit:vue": "vitest run",
    "test:watch:vue": "vitest",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:performance": "lighthouse-ci",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "lint": "eslint app/frontend/static/js --ext .js --cache --cache-location .cache/eslint",
    "lint:fix": "eslint app/frontend/static/js --ext .js --fix --cache --cache-location .cache/eslint",
    "audit": "npm audit",
    "validate": "npm run lint && npm run test"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "alpinejs": "^3.15.0",
    "concurrently": "^9.2.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.55.0",
    "@vue/test-utils": "^2.4.6",
    "vitest": "^1.6.0",
    "jsdom": "^24.1.0",
    "eslint-plugin-alpinejs": "^1.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^21.6.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.6",
    "vite": "^5.2.0",
    "@vitejs/plugin-vue": "^5.0.4"
  },
  "dependencies": {
    "vue": "^3.5.0",
    "chart.js": "^4.4.0",
    "htmx.org": "^2.0.7"
  },
  "keywords": [
    "lora",
    "ai",
    "machine-learning",
    "fastapi",
    "frontend",
    "htmx",
    "alpine",
    "tailwind",
    "pwa",
    "testing"
  ],
  "author": "LoRA Manager Team",
  "license": "MIT"
}
